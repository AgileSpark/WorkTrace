{
    "name": "WorkTrace",
    "description": "Chrome extension to save and manage job listings from various job posting sites",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
        "128": "./icon128.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "login.html"
    },
    "permissions": [
        "<all_urls>",
        "tabs",
        "contextMenus"
    ] 
}